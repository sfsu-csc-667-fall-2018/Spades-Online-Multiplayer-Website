{
  "name": "spades-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "NODE_ENV=development DEBUG=spades-server:* nodemon ./bin/www",
    "db:create:migration": "npx sequelize migration:generate --name ",
    "db:migrate": "npx sequelize db:migrate",
    "db:rollback": "npx sequelize db:migrate:undo",
    "postinstall": "npx sequelize db:migrate"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "pg-promise": "^10.4.4",
    "pug": "^2.0.4",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
